{
  "name": "little-window",
  "version": "1.0.0",
  "description": "A chatbot featured on Chayn websites, which quickly directs users to Chaynâ€™s relevant internal or external resources",
  "author": "Chayn",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "postinstall": "nest build",
    "build": "nest build",
    "heroku-postbuild": "npm run build-client",
    "build-client": "cd client && npm install && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "test": "NODE_ENV=test jest",
    "testtravis": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:leaks": "jest --detectOpenHandles",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/common/database-connection",
    "typeorm:migration": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fac-12/Little-Window.git"
  },
  "bugs": {
    "url": "https://github.com/fac-12/Little-Window/issues"
  },
  "homepage": "https://github.com/fac-12/Little-Window#readme",
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.9",
    "@nestjs/serve-static": "^2.1.0",
    "@nestjs/typeorm": "^7.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dialogflow": "^1.2.0",
    "dotenv": "^8.2.0",
    "nestjs-rollbar": "^1.3.0",
    "pg": "^8.0.3",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "storyblok-js-client": "^3.0.0",
    "typeorm": "^0.2.27"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint-config-prettier": "^6.10.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": "12.x"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "../jestSetup.ts"
    ]
  }
}
