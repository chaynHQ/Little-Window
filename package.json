{
  "name": "little-window",
  "version": "1.0.0",
  "description": "A chatbot featured on all Chayn websites in an iframe, which quickly directs users to Chaynâ€™s relevant internal or external resources",
  "main": "config.js",
  "scripts": {
    "start": "node ./src/server.js",
    "start:test": "NODE_ENV=production node ./src/server.js",
    "postinstall": "cd client && npm install && npm run build",
    "test": "NODE_ENV=test tape ./src/test/*test.js | tap-spec && cd client && npm run test",
    "server": "NODE_ENV=dev nodemon ./src/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm start"
  },
  "repository": {
    "type": "git",
